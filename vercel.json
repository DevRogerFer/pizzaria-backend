{
    "version": 2,
    "builds": [
        {
            "src": "src/server.ts",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "src/server.ts"
        }
    ],
    "env": {
        "DATABASE_URL": "@database_url",
        "JWT_SECRET": "@jwt_secret",
        "CLOUDINARY_NAME": "@cloudinary_name",
        "CLOUDINARY_KEY": "@cloudinary_key",
        "CLOUDINARY_SECRET": "@cloudinary_secret"
    }
}